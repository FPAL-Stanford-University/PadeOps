&INPUT
inputdir  =  "/scratch/users/jrwest/llnl_strength/stationary_interface"
outputdir  =  "/scratch/users/jrwest/llnl_strength/stationary_interface/data"

nx = 200                  ! Number of points in X
ny = 1                    ! Number of points in Y
nz = 1                    ! Number of points in Z

dt    = 1D-4              ! Fixed time step value (Set to negative to disable)
tstop = 2.0D-1            ! Physical time to stop the simulation
CFL   = .8D0             ! CFL criterion for calculating the time step (Set to negative to disable)
nsteps = -1               ! Number of time steps before stopping (Set to negative to disable)

tviz = 0.25D-1            ! Time between visualization dumps (Set to negative to disable)
vizprefix = "stationary_interface" ! Prefix for the visualization files

periodicx = .FALSE.       ! Periodic in X?
periodicy = .TRUE.        ! Periodic in Y?
periodicz = .TRUE.        ! Periodic in Z?

derivative_x = "cd10"     ! What derivative to use in X: "cd10", "cd06", "four", "cheb"
derivative_y = "cd10"     ! What derivative to use in Y: "cd10", "cd06", "four", "cheb"
derivative_z = "cd10"     ! What derivative to use in Z: "cd10", "cd06", "four", "cheb"

filter_x = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_y = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_z = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"

prow = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)
pcol = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)

/

&CINPUT
gam  = 1.4D0              ! Ratio of specific heats
Rgas = 1.D0               ! Gas constant
/

&IINPUT
nu = 0.0D0               ! Kinematic viscosity  !TODO: Check if correct
/

&SINPUT
gam     = 1.4D0           ! Ratio of specific heats
Rgas    = 1.D0            ! Gas constant
PInf    = 0D0            ! P_infinity, material stiffness
shmod   = 0d0            ! Shear modulus
PTeqb = .TRUE.            ! Yield strength of material
SOSmodel = .FALSE.            ! Yield strength of material
ns      = 2               ! Number of species

Cmu     = 2.D-3           ! Coefficient for artificial shear viscosity
Cbeta   = 1.D0            ! Coefficient for artificial bulk viscosity
Ckap    = 1.D-2           ! Coefficient for artificial thermal conductivity
Cdiff   = 3.D-3           ! Coefficient for artificial thermal conductivity
CY      = 1.D2            ! Coefficient for artificial thermal conductivity

x_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
x_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric
y_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
y_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric
z_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
z_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric

use_gTg  = .TRUE.          ! Use the symmetric Finger tensor formulation?
        
/

&PROBINPUT
gamma    = 1.67D0 ! Ratio of specific heats  -- ideal gas
Rgas     = 1.D0           ! Gas constant
p_infty  = 0D0           ! P_infinity, material stiffness --gas
mu       = 0D0           ! Shear modulus --gas
rho_0    = 1.D0           ! No deformation density
p_amb    = 1.D0          ! Ambient pressure
minVF    = 1.0D-10        ! Minimum mass fraction at initialization
rhoRatio = 5D0          ! Density ratio
pRatio   = 1D0          ! Density ratio
thick    = 3.0D0          ! Initial thickness in grid units for interface

gamma_2    = 1.67D0          ! Ratio of specific heats   -- aluminium
Rgas_2     = 1.D0           ! Gas constant
p_infty_2  = 0D0           ! P_infinity, material stiffness
mu_2       = 0D0           ! Shear modulus
rho_0_2    = 1.D0           ! No deformation density
plastic    = .false.         ! Logical to turn on plasticity
yield      = 8.79d-4        ! Yield strength of material
explPlast  = .FALSE.        ! Logical to switch between explicit (F) and implicit (T) plastic update
plastic2   = .false.         ! Logical to turn on plasticity
yield2     = 2.176d-3       ! Yield strength of material
explPlast2 = .FALSE.        ! Logical to switch between explicit (F) and implicit (T) plastic update
interface_init = 0.5d0     ! x location of interface at t = 0
kwave = 2.0D0              ! transverse wavelength (y)
/
