&INPUT

inputdir  =  "/home/akshays/Data/PadeOps/Multispecies_RM_1D"      ! Directory for input files
outputdir  = "/home/akshays/Data/PadeOps/Multispecies_RM_1D"      ! Directory for output files

nx = 400                  ! Number of points in X
ny = 1                    ! Number of points in Y
nz = 1                    ! Number of points in Z

dt = 1.0D-4               ! Fixed time step value (Set to negative to disable)
tstop = 0.1D0             ! Physical time to stop the simulation
CFL = 0.8D0               ! CFL criterion for calculating the time step (Set to negative to disable)
nsteps = -1               ! Number of time steps before stopping (Set to negative to disable)

