PInf    = 1.D0            ! P_infinity, material stiffness
shmod   = 1.D1            ! Shear modulus
PTeqb = .TRUE.            ! Yield strength of material
SOSmodel = .FALSE.            ! Yield strength of material
ns      = 2               ! Number of species
Cmu     = 2.D-3           ! Coefficient for artificial shear viscosity
Cbeta   = 1.D0            ! Coefficient for artificial bulk viscosity
Ckap    = 1.D-2           ! Coefficient for artificial thermal conductivity
Cdiff   = 2.D-4           ! Coefficient for artificial thermal conductivity
CY      = 1.D2           ! Coefficient for artificial thermal conductivity
x_bc1   = 0
x_bcn   = 0
y_bc1   = 0
y_bcn   = 0
z_bc1   = 0
z_bcn   = 0
/

&PROBINPUT
gamma    = 2.84D0         ! Ratio of specific heats  -- copper
Rgas     = 1.D0           ! Gas constant
p_infty  = 1.D0           ! P_infinity, material stiffness
mu       = 10.D0          ! Shear modulus
rho_0    = 1.D0           ! No deformation density
p_amb    = 5.D-2          ! Ambient pressure
rhoRatio = 1.0d0          ! Density ratio
muRatio  = 1.0d0          ! Shear modulus ratio
thick    = 3.0D0          ! Initial thickness of volume fraction discontinuity in grid units
shock_thick = 3.0d0       ! Initial thickness of velocity        discontinuity in grid units
minVF    = 1.0D-6         ! Minimum mass fraction at initialization
shock_init = 0.5D0        ! x location of shock     at t = 0
interface_init = 2.0D0    ! x location of interface at t = 0
normal    = .false.
sliding   = .true.
/
