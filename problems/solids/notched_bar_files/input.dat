&INPUT

inputdir  =  "./"      ! Directory for input files
outputdir  = "./out_notched_bar"  ! Directory for output files

nx = 301                   ! Number of points in X
ny = 101                    ! Number of points in Y
nz = 1                    ! Number of points in Z

dt    = 1.0D-10              ! Fixed time step value (Set to negative to disable)
tstop = 2.0d-8            ! Physical time to stop the simulation
CFL   = .5D0             ! CFL criterion for calculating the time step (Set to negative to disable)
nsteps = -1               ! Number of time steps before stopping (Set to negative to disable)

tviz = 1.0D-8             ! Time between visualization dumps (Set to negative to disable)
vizprefix = "notched_bar" ! Prefix for the visualization files

periodicx = .FALSE.       ! Periodic in X?
periodicy = .FALSE.        ! Periodic in Y?
periodicz = .TRUE.        ! Periodic in Z?

derivative_x = "cd10"     ! What derivative to use in X: "cd10", "cd06", "four", "cheb"
derivative_y = "cd10"     ! What derivative to use in Y: "cd10", "cd06", "four", "cheb"
derivative_z = "cd10"     ! What derivative to use in Z: "cd10", "cd06", "four", "cheb"

filter_x = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_y = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_z = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"

prow = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)
pcol = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)
/

&SINPUT
intSharp          = .FALSE.   ! Include interface sharpening terms
intSharp_spf      = .FALSE.   ! Use Shukla-Pantano-Freund method - not in divergence form
intSharp_cpg_west = .TRUE.   ! Jacob's version of kinematic coupling
intSharp_ufv = .TRUE.      ! Use second order finite volume for sharpening term
intSharp_pfloor = 1.D-12    ! Pressure floor in relaxation algorithm / LAD (LAD deprecated)

gam      = 1.4D0           ! Ratio of specific heats
Rgas     = 1.D0            ! Gas constant
PInf     = 1.D0            ! P_infinity, material stiffness
shmod    = 1.D1            ! Shear modulus
PTeqb    = .TRUE.          ! Pressure/temperature equilibrium model
PTeqb_new= .TRUE.          ! Pressure/temperature equilibrium model
pEqb    = .FALSE.          ! Pressure/temperature equilibrium model
pRelax  = .FALSE.          ! Pressure/temperature equilibrium model
SOSmodel = .FALSE.         ! Speed of sound model
ns       = 2               ! Number of species

intSharp_cpl = .TRUE.      ! Include coupling with momentum/energy eqns
intSharp_cpg = .TRUE.         ! Include coupling with kinematic eqn
intSharp_d02 = .TRUE.      ! Use second order
intSharp_msk = .TRUE.      ! Mask FV diffusion - no diffusion outside interface region
intSharp_flt = .FALSE.     ! Use dealliasing filter for interface sharpening terms
intSharp_gam = -2.0        ! Interface sharpening Gamma parameter
intSharp_eps = 3.90625D-3  ! 1.5625D-2 !3.125D-2 !1.5625D-2 !7.8125D-3 !3.90625D-3 !1.95313D-3 !32-64-128-256-512        ! Interface sharpening epsilon parameter
intSharp_cut = 1.0D-6      ! Interface sharpening VF cutoff parameter -- should = minVF
intSharp_dif = 1.D0        ! Interface sharpening VF out of bounds diffusion
intSharp_flp = .FALSE.     ! Filter pressure
intSharp_pfloor = 1.D-12    ! Pressure floor in relaxation algorithm / LAD (LAD deprecated)
intSharp_tfloor = 0.0      ! Temperature floor in LAD (LAD deprecated)
useOneG = .FALSE.	   ! Use mixture g formulation
use_gTg  = .TRUE.          ! Use the symmetric Finger tensor formulation?
strainHard = .TRUE.	   ! Solve the equations for strain hardening
cnsrv_g  = .FALSE.         ! Use the conservative form for ge equations
cnsrv_gt = .FALSE.         ! Use the conservative form for gt equations
cnsrv_gp = .FALSE.         ! Use the conservative form for gp equations
cnsrv_pe = .FALSE.         ! Use the conservative form for pe equations

Cmu      = 2.D-3           ! Coefficient for artificial shear viscosity
Cbeta    = 5.D0            ! Coefficient for artificial bulk viscosity
Ckap     = 1.D-2           ! Coefficient for artificial thermal conductivity
Cdiff    = 3.D-3           ! Coefficient for artificial material diffusivity
CY       = 1.D2            ! Coefficient for artificial material diffusivity
Cdiff_g  = 0.1             ! Coefficient for artificial g_e diffusivity
Cdiff_gt = 1.0 !0.1             ! Coefficient for artificial g_t diffusivity
Cdiff_gp = 1.0 !0.1             ! Coefficient for artificial g_p diffusivity
Cdiff_pe = 1.0             ! Coefficient for artificial pe diffusivity
Cdiff_pe_2 = 1.D2          ! Coefficient for artificial pe diffusivity

x_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
x_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric
y_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
y_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric
z_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
z_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric
/

