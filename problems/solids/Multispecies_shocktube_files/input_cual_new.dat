&INPUT

inputdir  =  "/home/nghaisas/runs/PadeOps/solid/msshtube"      ! Directory for input files
outputdir  = "/home/nghaisas/runs/PadeOps/solid/msshtube"    ! Directory for output files

nx = 101                  ! Number of points in X
ny = 1                    ! Number of points in Y
nz = 1                    ! Number of points in Z

dt = 1.0D-4               ! Fixed time step value (Set to negative to disable)
tstop = 0.20D0           ! Physical time to stop the simulation
