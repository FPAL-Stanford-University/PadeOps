&INPUT
outputdir  =  "./data"

nx=101                  ! Number of points in X
ny = 1                    ! Number of points in Y
nz = 1                    ! Number of points in Z

dt    = -5.0D-8              ! Fixed time step value (Set to negative to disable)
tstop = 1.0d-1            ! Physical time to stop the simulation
CFL   = .7D0             ! CFL criterion for calculating the time step (Set to negative to disable)
nsteps = -1               ! Number of time steps before stopping (Set to negative to disable)

tviz = 1.0d-2            ! Time between visualization dumps (Set to negative to disable)
vizprefix = "plasticShear" ! Prefix for the visualization files

periodicx = .false.       ! Periodic in X?
periodicy = .TRUE.        ! Periodic in Y?
periodicz = .TRUE.        ! Periodic in Z?

derivative_x = "cd10"     ! What derivative to use in X: "cd10", "cd06", "four", "cheb"
derivative_y = "cd10"     ! What derivative to use in Y: "cd10", "cd06", "four", "cheb"
derivative_z = "cd10"     ! What derivative to use in Z: "cd10", "cd06", "four", "cheb"

filter_x = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_y = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_z = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"

prow = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)
pcol = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)

&IINPUT
nu = 0.0D0               ! Kinematic viscosity  !TODO: Check if correct
/

&SINPUT
gam      = 2.00          ! Ratio of specific heats
Rgas     = 1.D0            ! Gas constant
PInf     = 1.0d0            ! P_infinity, material stiffness
shmod    = 0.2886D0            ! Shear modulus
PTeqb    = .TRUE.            ! Yield strength of material
SOSmodel = .FALSE.            ! Yield strength of material
ns       = 1               ! Number of species

Cmu     = 0.002d0 !0.002d0             ! Coefficient for artificial shear viscosity
Cbeta   = 5.0d0 !5d0             ! Coefficient for artificial bulk viscosity
Ckap    = 0.01d0 !0.01d0             ! Coefficient for artificial thermal conductivity
Cdiff   = 0d0             ! Coefficient for artificial thermal conductivity
CY      = 0d0             ! Coefficient for artificial thermal conductivity

x_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
x_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric
y_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
y_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric
z_bc1   = 0               ! 0: general, 1: symmetric/anti-symmetric
z_bcn   = 0               ! 0: general, 1: symmetric/anti-symmetric

use_gTg  = .FALSE.          ! Use the symmetric Finger tensor formulation?
        
strainHard = .true.	   ! Solve the equations for strain hardening
cnsrv_g  = .FALSE.         ! Use the conservative form for ge equations
cnsrv_gt = .FALSE.         ! Use the conservative form for gt equations
cnsrv_gp = .FALSE.         ! Use the conservative form for gp equations
cnsrv_pe = .FALSE.         ! Use the conservative form for pe equations
Cdiff_g  = 0.10d0 !0.1   !5.D-2! 1.D-2 !1.D-3 !1.D-6           ! Coefficient for artificial g_e diffusivity
Cdiff_gt = 0.10d0 !0.1   !5.D-2! 1.D-2 !1.D-3           ! Coefficient for artificial g_t diffusivity
Cdiff_gp = 0.10d0 !0.1   !5.D-2! 1.D-2 !1.D-2           ! Coefficient for artificial g_p diffusivity
Cdiff_pe = 0.10d0 !0.1   !1.D-1           ! Coefficient for artificial pe diffusivity
/

&PROBINPUT
gamma    = 2.0d0 ! Ratio of specific heats  -- ideal gas
p_infty  = 1.0d0
Rgas     = 1.D0           ! Gas constant
mu       = 0.2886d0           ! Shear modulus --gas
plastic    = .true.         ! Logical to turn on plasticity
yield      = 8.79d-4       ! Yield strength of material
explPlast  = .false.        ! Logical to switch between explicit (T) and implicit (F) plastic update
tau_0      = 1d-14
thick      = 0.01d
rho_0    = 1.0d0       ! No deformation density
rho_L    = 1.0d0       ! 
rho_R    = 1.0d0       !
p_L      = 1.0d5
p_R      = 1.0d5
u_L      = 0.0d0
u_R      = 0.0d0
v_L      = 1.0d-1       
v_R      = -1.0d-1
ge11_L   = 1.0D0
ge11_R   = 1.0d0
ge22_L   = 1.0d0
ge22_R   = 1.0d0
melt_t  = 4.52D0  !1.0E32     !melt temperature
melt_c  = 0.0 !0.21D0       !melt model coef
kos_b = 0.0D0
kos_t = 0.0D0
kos_h = 0.0D0
kos_g = 0.0D0
kos_m = 0.0D0
kos_q = 0.0D0
kos_f = 0.0D0
kos_alpha = 1.0D0
kos_beta  = 0.20D0
kos_e  = 0.0D0
kos_sh      = 0 !1 ! 1 g^t-g^e; 2 g^p-g^e; 3 pe
eta_det_ge  = 0.5  !0.5  !0.5!0.16667!0.1!0.001!0.01	       !density correction factor in g equations
eta_det_gp  = 0.5  !0.5  !1.0D-10!1.0D-10 !0.001 41
eta_det_gt  = 0.5 !0.25 !1.0D-2!0.01
diff_c_ge   = 0.00!0.25      !curl diffusion factor in g equations
diff_c_gp   = 0.00!0.25
diff_c_gt   = 0.00!0.25
interface_init = 5.0d-1
/
