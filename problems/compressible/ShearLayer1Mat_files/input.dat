&INPUT

inputdir  = "/home/nghaisas/runs/PadeOps_startup/compressible/shearlayer"
outputdir = "/home/nghaisas/runs/PadeOps_startup/compressible/shearlayer"

nx = 32             ! Number of points in X (256)
ny = 32             ! Number of points in Y (192)
nz = 32             ! Number of points in Z (128)

dt = -1.0D-4              ! Fixed time step value (Set to negative to disable)
tstop = 4D1              ! Physical time to stop the simulation
CFL = 0.5D0               ! CFL criterion for time step (Set to negative to disable)
nsteps = -1               ! Number of time steps before stopping (Set to negative to disable)

tviz = 1D-0    ! Time between visualization dumps (Set to negative to disable)
vizprefix = "shearlayer1mat"  ! Prefix for the visualization files
reduce_precision = .true. ! Reduce precision for I/O?

periodicx = .true.        ! Periodic in X?
periodicy = .false.       ! Periodic in Y?
periodicz = .true.        ! Periodic in Z?

derivative_x = "cd10"     ! derivative to use in X: "cd10", "cd06", "four", "cheb"
derivative_y = "cd10"     ! derivative to use in Y: "cd10", "cd06", "four", "cheb"
derivative_z = "cd10"     ! derivative to use in Z: "cd10", "cd06", "four", "cheb"

filter_x = "cf90"         ! filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_y = "cf90"         ! filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_z = "cf90"         ! filter to use in X: "cf90", "gaussian", "lstsq", "spectral"

xmetric = .false.         ! Streching in x-direction
ymetric = .true.          ! Streching in y-direction
zmetric = .false.         ! Streching in z-direction

prow = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)
pcol = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)

/

&CINPUT
gam   = 1.4D0                        ! Ratio of specific heats
Rgas  = 1.D0                         ! Gas constant
Cmu   = 0.D0                         ! LAD coeff  
Cbeta = 0.D0                         ! LAD coeff 
Ckap  = 0.D0                         ! LAD coeff 
Cdiff = 0.D0                         ! LAD coeff 
Cy    = 0.D0                         ! LAD coeff 
inviscid = .false.                   ! Inviscid simulation?
ns       = 1                         ! Number of species + passive tracers
nrestart = 5000                      ! Number of steps between restart dumps
compute_tke_budget = .false.         ! Compute TKE budgets?
useSGS   = .false.                   ! SGS Model
y_bc1    = 1                         ! Left  Y BC (0 => general, 1 => symmetric). Not used if periodic
y_bcn    = 1                         ! Right Y BC (0 => general, 1 => symmetric). Not used if periodic
/

&SGS_MODEL
DynamicProcedureType     =  1        !0=Static  1=Local_mgm  2=Global_mgm
SGSmodelID               =  2        !0=Smagorinsky  1=sigma 2=AMD 3=MGM
useWallDamping           =  .false. 
ncWall                   =  1.0D0
Csgs                     =  1.3D0
Ctke                     =  0.1D0
DynProcFreq              =  1
useSGSDynamicRestartFile =  .false.
DomainAveraged_DynProc   =  .false.
SGSDynamicRestartFile    =  "null"
filter_in_x              =  .true. 
filter_in_y              =  .true.
filter_in_z              =  .true.
testfilter_x             =  "box2"
testfilter_y             =  "box2"   
testfilter_z             =  "box2"
DeltaRatio               =   2.0D0
isEddyViscosityModel     =  .true.
isEddyDiffmodel          =  .true.
isTurbPrandtlconst       =  .false.
useDynamicProcedureScalar=  .false.
PrSGS                    =   0.7D0
/

&PROBINPUT
Lx      = 68.D0         ! Pantano 2002        
Ly      = 59.D0         ! Pantano 2002
Lz      = 68.D0         ! Pantano 2002
Mc      = 0.6D0         ! Convective Mach number
Re      = 5.0D1         ! Reynolds number rho_Ref*dU*L/mu_ref
Pr      = 0.7D0         ! Prandtl number nu/kappa 0.7
Sc      = 1.D0          ! Schmidt number nu/massDiffusivity
T_ref = 1.D0            ! Reference temp 
p_ref = 1.D0            ! Reference press 
rho_ref = 1.D0          ! Reference density
rho_ratio = 1.D0        ! Density ratio rho2/rho1
noiseAmp = 0.D0         ! noise amplitude 
fname_prefix = "/home/nghaisas/runs/PadeOps_startup/compressible/shearlayer"
use_lstab    = .false.
no_pert      = .true. 
/

&METRICS
xmetric_flag = 1        !flags: 1=conc towards centre, 2=conc towards both ends, 3=arbitrary, 4=finite-difference evaluation of metrics
ymetric_flag = 1
zmetric_flag = 1
metric_params(2,1) = 0.0D0      !focus 
metric_params(2,2) = 2.0D0      !stretching parameter :tau 
metric_params(2,3) = -100.0D0   !start: -Ly/2
metric_params(2,4) = 100.0D0    !hh: Ly/2
/

