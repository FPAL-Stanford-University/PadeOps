&INPUT

inputdir  =  "/home/vsj/Codes/shocktube/shocktube2d_dimen"      ! Directory for input files
outputdir  = "/home/vsj/Codes/shocktube/shocktube2d_dimen"      ! Directory for output files

nx = 1065                 ! Number of points in X
ny = 49                   ! Number of points in Y
nz = 1                    ! Number of points in Z

dt = -1.0D-4              ! Fixed time step value (Set to negative to disable)
tstop = 0.006D0           ! Physical time to stop the simulation
CFL = 0.7D0               ! CFL criterion for calculating the time step (Set to negative to disable)
nsteps = -1               ! Number of time steps before stopping (Set to negative to disable)

tviz = 7.5D-6             ! Time between visualization dumps (Set to negative to disable)
vizprefix = "shocktube"   ! Prefix for the visualization files
reduce_precision  = .false.

periodicx = .FALSE.       ! Periodic in X?
periodicy = .FALSE.       ! Periodic in Y?
periodicz = .FALSE.       ! Periodic in Z?

derivative_x = "cd10"     ! What derivative to use in X: "cd10", "cd06", "four", "cheb"
derivative_y = "cd10"     ! What derivative to use in Y: "cd10", "cd06", "four", "cheb"
derivative_z = "cd10"     ! What derivative to use in Z: "cd10", "cd06", "four", "cheb"

filter_x = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_y = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"
filter_z = "cf90"         ! What filter to use in X: "cf90", "gaussian", "lstsq", "spectral"

xmetric = .false.         ! Streching in x-direction
ymetric = .false.         ! Streching in y-direction
zmetric = .false.         ! Streching in z-direction

prow = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)
pcol = 0                  ! Number of rows in 2D processor decomposition (set 0 for auto-tuning)

SkewSymm   = .false.
/

&CINPUT
ns   = 1
gam  = 1.4D0              ! Ratio of specific heats
Rgas = 287.053D0          ! Ratio of specific heats
Cmu   = 0.002D0           ! LAD coeff  
Cbeta = 1.75D0            ! LAD coeff 
Ckap  = 0.01D0            ! LAD coeff 
Cdiff = 0.003D0           ! LAD coeff 
Cy    = 1D2               ! LAD coeff 
inviscid = .false.        
nrestart = 5000           ! Number of steps between restart dumps
x_bc1 = 1                 ! Left  X boundary ( 0 => general, 1 => symmetric ) 
x_bcn = 1                 ! Right X boundary ( 0 => general, 1 => symmetric ) 
y_bc1 = 1                 ! Left  Y boundary ( 0 => general, 1 => symmetric ) 
y_bcn = 1                 ! Right Y boundary ( 0 => general, 1 => symmetric ) 
compute_tke_budget = .false.
compute_scale_decomposition = .false.
forcing = .false
useSGS   = .false.
/

&SGS_MODEL
DynamicProcedureType     =  1        !0=Static  1=Local_mgm  2=Global_mgm
SGSmodelID               =  2        !0=Smagorinsky  1=sigma 2=AMD 3=MGM
useWallDamping           =  .false. 
ncWall                   =  1.0D0
Csgs                     =  1.3D0
Ctke                     =  0.1D0
DynProcFreq              =  1
useSGSDynamicRestartFile =  .false.
DomainAveraged_DynProc   =  .false.
SGSDynamicRestartFile    =  "null"
filter_in_x              =  .true. 
filter_in_y              =  .true.
filter_in_z              =  .true.
testfilter_x             =  "box2"
testfilter_y             =  "box2"   
testfilter_z             =  "box2"
DeltaRatio               =   2.0D0
isEddyViscosityModel     =  .true.
isEddyDiffmodel          =  .true.
isTurbPrandtlconst       =  .false.
useDynamicProcedureScalar=  .false.
PrSGS                    =   0.7D0

/
&PROBINPUT
ns    = 1               ! No.of species(initfields)
Lx    = 6.4D0           
Ly    = 0.064D0
Lz    = 0.01D0
Pr    = 0.7D0
Sc    = 1.0D0
gam   = 1.4D0          ! specific heat ratio (initfields)
Rgas  = 287.053D0      ! Gas constant (initfields)
mu_ref= 1.849D-5      
pLef  = 101325D1       ! Pressure at left side of diaphragm
pRig  = 101325D0       ! Pressure at right side of diaphragm
rhoLef= 11.851D0       ! Density at left side of diaphragm
rhoRig= 1.1851D0       ! Density at right side of diaphragm
/
